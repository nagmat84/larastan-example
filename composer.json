{
    "name": "nagmat84/larastan-example",
    "type": "project",
    "description": "Example app to demonstrate Larastan errors.",
    "keywords": ["laravel", "larastan", "mockup"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
		"nunomaduro/larastan": "^2.0",
		"phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse"
		]
	},
    "extra": {
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
